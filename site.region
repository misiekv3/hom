<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js"></script>

    <!-- bootstrap -->
    <squarespace:script src="bootstrap.min.js" />

    <!--Include sqs scripts, meta tags, and user content from the code injection tab.-->
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">

    <div class="container-own">

    <!-- header -->
<div id="sticky-header">
    <div class="container">
      <div class="row">
        <div id="logo"><a href="index.html" title="Homepage"><img src="assets/hom_logo.png" class="img-responsive" alt="" /></a></div>
        
        <div id="burger">
          <svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
            viewBox="0 0 36 20" enable-background="new 0 0 36 20" xml:space="preserve">
            <path fill-rule="evenodd" clip-rule="evenodd" fill="#FFFFFF" d="M0,0h36v3H0V0z"/>
            <path fill-rule="evenodd" clip-rule="evenodd" fill="#FFFFFF" d="M0,8h36v3H0V8z"/>
            <path fill-rule="evenodd" clip-rule="evenodd" fill="#FFFFFF" d="M0,17h36v3H0V17z"/>
          </svg>
        </div>
        
        <div id="x-close">
          <svg version="1.1" id="Layer_2" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
            viewBox="0 0 28 28" enable-background="new 0 0 28 28" xml:space="preserve">
            <path fill-rule="evenodd" clip-rule="evenodd" fill="#FFFFFF" d="M0,25.8L25.8,0L28,2.2L2.2,28L0,25.8z"/>
            <path fill-rule="evenodd" clip-rule="evenodd" fill="#FFFFFF" d="M2.2,0L28,25.8L25.8,28L0,2.2L2.2,0z"/>
          </svg>
        </div>
        
        <div id="menu" class="col-lg-6 col-md-8 col-sm-9 col-xs-12 right">
          <ul>
            <li class="active"><a data-scroll data-options='{ "offset": 0 }' href="#home">Home</a></li>
            <li><a data-scroll data-options='{ "offset": 100 }' href="#services">Services</a></li>
            <li><a data-scroll data-options='{ "offset": 100 }' href="#amenities">Amenities</a></li>
            <li><a data-scroll data-options='{ "offset": 100 }' href="#gallery">Gallery</a></li>
            <li><a data-scroll data-options='{ "offset": 100 }' href="#location">Location</a></li>
            <li><a data-scroll data-options='{ "offset": 100 }' href="#floorplans">Floorplans</a></li>
            <li><a data-scroll data-options='{ "offset": 100 }' href="#contact">Contact</a></li>
          </ul>
        </div>
      </div>
    </div>
  </div>

<!-- slider banner -->

<div id="home" class="section">
    <div id="scroll-down">
      <svg version="1.1" id="Layer_3" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
         viewBox="0 0 492.3 492.3" enable-background="new 0 0 492.3 492.3" xml:space="preserve">
      <path fill="#FFFFFF" d="M260.6,375.3c-8,8-20.8,8-28.8,0L86.2,229.8c-8-8-8-20.8,0-28.8l32.7-32.7c8-8,20.8-8,28.8,0l98.4,98.4
        l98.4-98.4c8-8,20.8-8,28.8,0l32.7,32.7c8,8,8,20.8,0,28.8L260.6,375.3z M492.3,246.1C492.3,110.3,382,0,246.1,0
        C110.2,0,0,110.3,0,246.1s110.2,246.1,246.1,246.1C382,492.3,492.3,382,492.3,246.1z"/>
      </svg>
    </div>
    
    <div id="header-slider">
      <div class="item">
        <div class="container">
          <div class="headline txt-center">
            <h1>Your Life, Your Terms</h1>
            <div class="separator"></div>
            <p>HōM is a new boutique apartment community in Silverlake. We created it to have everything you need, and nothing you don’t - luxury living at a fraction of the cost.</p>
          </div>
        </div>
        
        <img src="assets/header_photo_1.jpg" class="img-responsive" alt=""/>
      </div>
      
      <div class="item">
        <div class="container">
          <div class="headline txt-center">
            <h1>Your Life, Your Terms</h1>
            <div class="separator"></div>
            <p>HōM is a new boutique apartment community in Silverlake. We created it to have everything you need, and nothing you don’t - luxury living at a fraction of the cost.</p>
          </div>
        </div>
        
        <img src="assets/header_photo_1.jpg" class="img-responsive" alt=""/>
      </div>
    </div>
  </div>


      <!--Content injection point.-->
      <main id="content" class="content" role="main" data-content-field="main-content">
       {squarespace.main-content}
      </main>

      <!--Footer with open block field.-->
      <footer id="footer" class="footer">
        <squarespace:block-field id="footerBlocks" columns="12" />
      </footer>

    </div>

    <!--Combo and minify the scripts when you're not logged in.-->
    <!--<squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" /> -->

    <!--Injection point for tracking scripts and user content from the code injection tab.-->
    {squarespace-footers}
    
    <squarespace:script src="owl.carousel.min.js"/>
  
    <squarespace:script src='smooth-scroll.js'/>
    <script>
      smoothScroll.init({
      speed: 500,
      easing: 'easeInOutCubic',
      offset: 0,
      updateURL: false,
      callbackBefore: function ( toggle, anchor ) {},
      callbackAfter: function ( toggle, anchor ) {}
      });
    </script>
    <squarespace:script src="main.js" />
  </body>
</html> 
